{
  "name": "lifekey-server",
  "version": "0.0.0",
  "description": "",
  "private": true,
  "main": "src/init",
  "scripts": {
    "test": "NODE_ENV=development istanbul test _mocha -- --recursive --bail --no-exit -R spec",
    "start": "NODE_ENV=development node src/init",
    "stn": "NODE_ENV=stn node src/init",
    "local": "NODE_ENV=local node src/init",
    "todo": "leasot --skip-unsupported --ignore=node_modules/**/* {test,src}/**/*.js; exit 0",
    "db:update": "bin/docker_database_pull",
    "db:start": "NODE_ENV=development bin/docker_database_run; until npm run db:reset; do :; done;",
    "db:stop": "bin/docker_database_stop",
    "db:create": "NODE_ENV=development bin/database_tables_create",
    "db:drop": "NODE_ENV=development bin/database_tables_drop",
    "db:reset": "npm run db:drop && npm run db:create",
    "db:truncate": "NODE_ENV=development bin/database_tables_truncate",
    "db:proxy": "./bin/cloud_sql_proxy -credential_file=etc/keys/dev.json -instances=vivid-case-125013:europe-west1:lifekey-staging=tcp:3306"
  },
  "author": "Phillip Gibb <phillip.gibb@trustlab.tech>",
  "license": "UNLICENSED",
  "dependencies": {
    "biguint-format": "^1.0.0",
    "blocked": "^1.2.1",
    "body-parser": "^1.15.2",
    "cors": "^2.8.1",
    "cuid": "^1.3.8",
    "eccrypto": "^1.0.3",
    "ethereumjs-tx": "^1.3.5",
    "ethereumjs-util": "^5.2.0",
    "express": "^4.14.0",
    "fluster": "0.0.15",
    "identity-service-wrapper": "git@github.com:consent-global/identity-service-wrapper.git",
    "lifekey-crypto": "git@github.com:consent-global/lifekey-crypto.git",
    "indy-sdk": "^1.5.0-beta",
    "jsonld": "^0.4.11",
    "ld-query": "^1.0.0",
    "lodash": "^4.17.10",
    "morgan": "^1.7.0",
    "mysql2": "^1.5.3",
    "qrcode": "^0.9.0",
    "secp256k1": "^3.2.5",
    "sendgrid": "^4.7.1",
    "sequelize": "^4.38.0",
    "web3": "^0.19.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "istanbul": "^0.4.5",
    "leasot": "^4.3.1",
    "mocha": "^3.2.0"
  }
}
